{
  "manifest_version": 2,
  "name": "AngularJS Base",
  "version": "1.0",
  "description": "AngularJS Base Project.",
  "icons": {
    "16": "img/icon_16.png",
    "128": "img/icon_128.png"
  },
  "browser_action": {
    "default_icon": "img/icon_16.png",
    "default_title": "AngularJS Base",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "permissions": [
    "tabs"
  ]
}
